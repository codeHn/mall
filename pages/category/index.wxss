page {
  height: 100%;
}
page .cates {
  height: 100%;
}
page .cates .cates_container {
  height: calc(100vh - 75rpx);
  display: flex;
}
page .cates .cates_container scroll-view {
  height: calc(100vh - 75rpx);
}
page .cates .cates_container .left_menu {
  flex: 2;
}
page .cates .cates_container .left_menu .active {
  color: var(--themeColor);
  border-left: 5px solid currentColor;
}
page .cates .cates_container .left_menu .menu_item {
  height: 80rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 30rpx;
}
page .cates .cates_container .right_content {
  flex: 5;
}
page .cates .cates_container .right_content .goods_group .goods_title {
  height: 80rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
page
  .cates
  .cates_container
  .right_content
  .goods_group
  .goods_title
  .delimiter {
  color: #ccc;
  padding: 0 10rpx;
}
page .cates .cates_container .right_content .goods_group .goods_list {
  display: flex;
  flex-wrap: wrap;
}
page .cates .cates_container .right_content .goods_group .goods_list navigator {
  width: 33.33%;
  text-align: center;
}
page
  .cates
  .cates_container
  .right_content
  .goods_group
  .goods_list
  navigator
  image {
  width: 50%;
}
